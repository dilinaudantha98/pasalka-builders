{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "src": "/api/(.*)",
      "dest": "/api/"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
